<%- include('../base/top') %>

<h1>Delete Jukebox</h1>

<h2>Are you sure you want to delete this jukebox?</h2>
<form method="post" action="/api/jukebox/delete" enctype="application/x-www-form-urlencoded">
    <label for="id">Id</label>
    <input name="id" type="text" value="<%= jukebox.id %>" readonly>

    <label for="displayName">Display Name</label>
    <input name="displayName" type="text" value="<%= jukebox.displayName %>" readonly>

    <input type="submit" value="Delete Jukebox">
</form>

<%- include('../base/bottom') %>
