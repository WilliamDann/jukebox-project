<div class="search-result">
    
    <a class="link-success" href="/suggest?suggestTo=<%= suggestTo %>&uri=<%= item.uri %>">
        <img style="width: 100px" src="<%= item.album.images[0].url %>">
        <p>Title: <%= item.name%></p>
        <p>Artist: <%= item.artists[0].name%></p>
        <!-- <p>URI: <%= item.uri %></p> -->

    </a>
</div>

